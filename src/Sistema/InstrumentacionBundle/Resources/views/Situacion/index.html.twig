{% extends '::base.html.twig' %}
{% block javascripts %}
<script>
                         
        function Validar()
        {
             var contrato = $('#selectcampo').val();
             if(contrato == 0)
                 {
                     alert("Seleccione un campo.");
                     return false;
                 }
             $('#formbuscador').submit();
        }
       
</script>
{{ parent() }}

<script> 
    jQuery(document).ready(function() {       
           // initiate layout and plugins
           App.init();
           ComponentsDropdowns.init();
           ComponentsPickers.init();
     });   
</script>


{% endblock %}
{% block body %}


<div class=" container">
    

           <div class=" col-lg-12">
      <div class=" page-header margin-none">          
                    <h2 class="padding-none">Situacion</h2>         
      </div> 
   <div class="table-responsive">
       <table class="table table-striped table-hover">
        <thead>
                      <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
                                  {% for entity in entities %}
                         <tr>         
                <td>{{ entity.id }}</td>         
                <td>{{ entity.situaciones }}</td>
                <td>
                  
                        
                                                <a href="{{ path('situacion_show', { 'id': entity.id }) }}"   class="btn btn-sm btn-info"  >show</a>
                  
                        
                                                <a href="{{ path('situacion_edit', { 'id': entity.id }) }}"  class="btn btn-sm btn-primary"  >edit</a>
                        
               
                </td>
            </tr>
        {% endfor %}
           
           
        </tbody>
    </table>
                                 <ul>
                                <li>
                                    <a href="{{ path('situacion_new') }}">
                                        Create a new entry
                                    </a>
                                </li>
                            </ul>
                      
        
      </div>
    </div>
</div>
    
{% endblock %}
