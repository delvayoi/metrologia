{% extends '::base.html.twig' %}
{% block javascripts %}
<script>
                         
        function Validar()
        {
             var contrato = $('#selectcampo').val();
             if(contrato == 0)
                 {
                     alert("Seleccione un campo.");
                     return false;
                 }
             $('#formbuscador').submit();
        }
       
</script>
{{ parent() }}

<script> 
    jQuery(document).ready(function() {       
           // initiate layout and plugins
           App.init();
           ComponentsDropdowns.init();
           ComponentsPickers.init();
     });   
</script>


{% endblock %}
{% block body %}


<div class=" container">
 
           <div class=" col-lg-12">
      <div class=" page-header margin-none">          
                    <h2 class="padding-none">Instrumentos dados de baja</h2>         
      </div> 
   <div class="table-responsive">
       <table class="table table-striped table-hover">
        <thead>
                      <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Codigo</th>
                <th>Rango</th>
                <th>Modelo</th>
                <th>Situacion</th>
                <th>Magnitud</th>
                <th>Area</th>
                <th>Exactitud</th>
                <th>Equipo</th>
               
                
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
           {% for resultado in resultado %}
          <tr>         
                <td>{{ resultado.id }}</td>         
                <td>{{ resultado.nombre   }}</td>         
                <td>{{ resultado.codigo }}</td>         
                <td>{{ resultado.rango}}</td>        
                <td>{{ resultado.modelo }}</td>
                <td>{{ resultado.situaciones }}</td>
                <td>{{ resultado.magnitudes}}</td>
                <td>{{ resultado.area}}</td>
                 <td>{{ resultado.exactitudes }}</td>
              
              
                <td>
                  
                   
                                                <a href="{{ path('instrumento_show', { 'id': resultado.id }) }}"   class="btn btn-sm btn-info"  >show</a>
                  
                        
                                                <a href="{{ path('instrumento_edit', { 'id': resultado.id }) }}"  class="btn btn-sm btn-primary"  >edit</a>
                             
                                            
                </td>
            </tr>
        {% endfor %}
           
           
        </tbody>
    </table>
                                 <ul>
                                <li>
                                    <a href="{{ path('instrumento_new') }}">
                                        Create a new entry
                                    </a>
                                </li>
                            </ul>
                      
       
        
      </div>
    </div>
</div>
    
{% endblock %}
