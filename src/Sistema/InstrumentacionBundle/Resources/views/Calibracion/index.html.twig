{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Calibracion list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fechaultima</th>
                <th>Fechareal</th>
                <th>Fechaproxima</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('calibracion_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
              
                  
                  
                <td>{% if entity.fechaultima %}{{ entity.fechaultima|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechareal %}{{ entity.fechareal|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechaproxima %}{{ entity.fechaproxima|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('calibracion_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('calibracion_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('calibracion_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
