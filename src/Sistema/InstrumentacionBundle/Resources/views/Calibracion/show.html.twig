{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Calibracion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Fechaultima</th>
                <td>{{ entity.fechaultima|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechareal</th>
                <td>{{ entity.fechareal|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechaproxima</th>
                <td>{{ entity.fechaproxima|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('calibracion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('calibracion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
